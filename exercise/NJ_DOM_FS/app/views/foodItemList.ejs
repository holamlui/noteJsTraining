<!DOCTYPE html>
<html>

<head>
  <title>Food Store</title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
  <table>
    <tr>
      <th>Food Name</th>
      <th>Price</th>
      <th>BestBefore</th>
    </tr>
    <% for (var i = 0; i < result.length; i++){ %>
    <tr>
    <td><%= result[i].name %>
      </td>
      <td>
        <%= result[i].price %>
      </td>
      <td>
        <%= result[i].bestBefore %>
      </td>
      </tr>
      <%} %>
  </table>
</body>

</html>